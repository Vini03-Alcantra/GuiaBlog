<%- include('../../partials/header') -%>
<%- include('../../partials/navbar') -%>

<div class="container">
    <hr>
    <h2>Categorias</h2>
    <a class="btn btn-success" href="/admin/users/create">Cadastrar</a>
    <hr>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <th><%= user.id %></th>
                    <th><%= user.email %></th>                                        
                </tr>
            <% }) %>
        </tbody>
    </table>
</div>


<%- include('../../partials/footer') -%>